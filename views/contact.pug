                  a(href='#') yoursite.com
          .col-md-1
          .col-md-6.ftco-animate
            form.contact-form(action='#')
              .row
                .col-md-6
                  .form-group
                    input.form-control(type='text', placeholder='Your Name')
                .col-md-6
                  .form-group
                    input.form-control(type='text', placeholder='Your Email')
              .form-group
