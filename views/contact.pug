  body
    nav#ftco-navbar.navbar.navbar-expand-lg.navbar-dark.ftco_navbar.bg-dark.ftco-navbar-light
      .container
        a.navbar-brand(href='index.html')
          | Coffee
          small Blend
        button.navbar-toggler(type='button', data-toggle='collapse', data-target='#ftco-nav', aria-controls='ftco-nav', aria-expanded='false', aria-label='Toggle navigation')
          span.oi.oi-menu
          |  Menu
        #ftco-nav.collapse.navbar-collapse
          ul.navbar-nav.ml-auto
            li.nav-item
              a.nav-link(href='index.html') Home
            li.nav-item
              a.nav-link(href='menu.html') Menu
            li.nav-item
              a.nav-link(href='services.html') Services
            li.nav-item
              a.nav-link(href='blog.html') Blog
            li.nav-item
              a.nav-link(href='about.html') About
            li.nav-item.dropdown
              a#dropdown04.nav-link.dropdown-toggle(href='room.html', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false') Shop
              .dropdown-menu(aria-labelledby='dropdown04')
                a.dropdown-item(href='shop.html') Shop
                a.dropdown-item(href='product-single.html') Single Product
                a.dropdown-item(href='room.html') Cart
                a.dropdown-item(href='checkout.html') Checkout
            li.nav-item.active
              a.nav-link(href='contact.html') Contact
            li.nav-item.cart
              a.nav-link(href='cart.html')
                span.icon.icon-shopping_cart
                span.bag.d-flex.justify-content-center.align-items-center
                  small 1
    // END nav
    section.home-slider.owl-carousel
      .slider-item(style='background-image: url(images/bg_3.jpg);', data-stellar-background-ratio='0.5')
        .overlay
        .container
          .row.slider-text.justify-content-center.align-items-center
            .col-md-7.col-sm-12.text-center.ftco-animate
              h1.mb-3.mt-5.bread Contact Us
              p.breadcrumbs
                span.mr-2
                  a(href='index.html') Home
                span Contact
    section.ftco-section.contact-section
      .container.mt-5
        .row.block-9
          .col-md-4.contact-info.ftco-animate
            .row
              .col-md-12.mb-4
